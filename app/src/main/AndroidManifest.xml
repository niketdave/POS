<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.niket.foodorder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/floratine"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Splesh_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".First_page"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Main_Activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/Details"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".About_us" />
        <activity
            android:name=".Menu_final"
            android:label="MENU"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".FeedBack"
            android:label="FeedBack"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".order"
            android:label="Order"
            android:theme="@style/Base.Theme.AppCompat"  />
        <activity android:name=".item_details"
            android:label="Item Details"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".Order_details"
            android:label="Order Details"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".Rate_us"
            android:label="Rate US"
            android:theme="@style/Base.Theme.AppCompat" ></activity>
    </application>

</manifest>